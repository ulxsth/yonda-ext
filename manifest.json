{
  "manifest_version": 3,
  "name": "yonda!",
  "version": "1.1",
  "description": "よんだことをツイートします",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icon-128x128.png"
    }
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ]
}
